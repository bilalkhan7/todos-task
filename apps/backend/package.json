{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --project tsconfig.json src/index.ts",
    "build": "tsc -p tsconfig.json && npx prisma generate --schema=prisma/schema.prisma",
    "start": "node dist/index.js",
    "prisma:generate": "npx prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --name init --schema=prisma/schema.prisma",
    "migrate:test": "prisma migrate deploy --schema prisma/schema.prisma --env-file .env.test",
    "migrate:deploy": "npx prisma migrate deploy --schema=prisma/schema.prisma",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^2.4.3",
    "connect-pg-simple": "^9.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "1.11.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/csurf": "^1.11.5",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.14",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "prisma": "^6.16.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
